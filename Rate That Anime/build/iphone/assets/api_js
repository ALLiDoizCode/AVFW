var getData=function(){var e="https://hbrd-v1.p.mashape.com/users/"+username.value+"/library";if(Ti.Network.online){var t=Ti.Network.createHTTPClient();t.open("GET",e),t.setRequestHeader("X-Mashape-Authorization","aAlcvjLDMSmshMRRPNyBJlkHR8wYp1s9KkfjsnXOf5ppTcZ4yO"),t.send(),t.onerror=function(){Ti.UI.createAlertDialog({title:"Network Error",message:"unable to retrieve data"}).show()},t.onload=function(){alert("api loaded");for(var e=JSON.parse(this.responseText),t=0;t<e.length;t++){var i=Ti.UI.createImageView({image:e[t].anime.cover_image,title:e[t].anime.slug,timeStart:e[t].anime.started_airing,timeEnd:e[t].anime.finished_airing,desc:e[t].anime.synopsis,score:e[t].anime.community_rating,height:150,top:170*t});i.addEventListener("touchstart",function(e){mainImage.image=e.source.image,title.title=e.source.title,year.text=e.source.timeStart,label.text=e.source.desc,rating.text=Math.round(100*e.source.score)/100}),scroll.add(i)}}}};exports.getData=getData;
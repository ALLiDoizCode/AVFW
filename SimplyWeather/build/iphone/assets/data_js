var Cloud=require("Cloud"),create=function(e,t,a){var o=Ti.Database.open("weather");o.execute("CREATE TABLE IF NOT EXISTS dataTbl (id INTEGER PRIMARY KEY, temp TEXT, city TEXT, desc TEXT)"),o.execute("INSERT INTO dataTbl (temp, city, desc) VALUES (?, ?, ?)",e,t,a);o.lastInsertRowId;o.close(),read()};exports.create=create;var read=function(){tableData=[];for(var e=Ti.Database.open("weather"),t=e.execute("SELECT temp, city, desc FROM dataTbl");t.isValidRow();)tableData.push({temp:t.fieldByName("temp"),city:t.fieldByName("city"),desc:t.fieldByName("desc")}),t.next();t.close(),e.close();var a=tableData[0].city,o=tableData[0].temp,i=tableData[0].desc;tempLabel.text=o,cityLabel.text=a,descLabel.text=i,Cloud.loginApp(o,a,i)},readCloud=function(){tableData=[];for(var e=Ti.Database.open("weather"),t=e.execute("SELECT temp, city, desc FROM dataTbl");t.isValidRow();)tableData.push({temp:t.fieldByName("temp"),city:t.fieldByName("city"),desc:t.fieldByName("desc")}),t.next();t.close(),e.close();var a=tableData[0].city,o=tableData[0].temp,i=tableData[0].desc;Cloud.loginApp(o,a,i)};exports.read=read,exports.readCloud=readCloud;
var create=function(e,t,a){var o=Ti.Database.open("weather");o.execute("CREATE TABLE IF NOT EXISTS dataTbl (temp TEXT, city TEXT, desc TEXT)"),o.execute("INSERT INTO dataTbl (temp, city, desc) VALUES (?, ?, ?)",e,t,a);o.lastInsertRowId;o.close()};exports.create=create;var read=function(){tableData=[];for(var e=Ti.Database.open("weather"),t=e.execute("SELECT temp, city, desc FROM dataTbl");t.isValidRow();)tableData.push({temp:t.fieldByName("temp"),city:t.fieldByName("city"),desc:t.fieldByName("desc")}),console.log(tableData.temp),t.next();return t.close(),e.close(),tableData};exports.read=read;